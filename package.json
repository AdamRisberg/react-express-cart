{
  "name": "react-express-cart",
  "version": "0.1.0",
  "description": "React and Express based shopping cart system.",
  "main": "index.js",
  "scripts": {
    "client-build": "cd client && npm run build",
    "client-test": "cd client && npm run test",
    "client": "cd client && npm start",
    "server": "nodemon index.js --ignore client/ --ignore settings/",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "set NODE_ENV=production&& npm run server"
  },
  "author": "Adam Risberg <myniztan@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "date-fns": "^1.29.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "formidable": "^1.2.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.13",
    "mongoose-paginate": "^5.0.3",
    "nodemailer": "^4.6.8",
    "sharp": "^0.21.0",
    "stripe": "^6.12.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9"
  }
}
